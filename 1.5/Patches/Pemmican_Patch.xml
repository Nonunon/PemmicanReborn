<Patch>
    <!-- Add Pemmican Reborn recipes to cooking stuff. -->
    <Operation Class="PatchOperationAdd">
        <xpath>Defs/ThingDef[defName="FueledStove" or defName="ElectricStove" or defName="Campfire"]/recipes</xpath>
		<value>
			<li>Make_Pemmican_Reborn</li>
			<li>Make_PemmicanBulk_Reborn</li>
		</value>
	</Operation>
</Patch>
